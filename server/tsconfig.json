{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    // "module": "ESNext",
    "strict": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
